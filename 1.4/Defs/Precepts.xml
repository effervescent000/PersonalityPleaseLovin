<?xml version="1.0" encoding="UTF-8"?>
<Defs>
  <!-- succubi related -->
  <IssueDef>
    <defName>PP_SuccubiIssue</defName>
    <label>succubi</label>
    <iconPath>UI/Issues/PhysicalLove</iconPath>
  </IssueDef>

  <PreceptDef>
    <defName>PP_SuccubiRevered</defName>
    <issue>PP_SuccubiIssue</issue>
    <label>revered</label>
    <impact>Medium</impact>
    <description>Succubi are sacred, and the pleasures they bring are divine.</description>
    <comps>
      <li Class="PreceptComp_SituationalThought">
        <thought>PP_SuccubiRevered_Opinion</thought>
        <description>Succubus</description>
      </li>
      <li Class="PreceptComp_SelfTookMemoryThought">
        <eventDef>PP_GotLovinSuccubus</eventDef>
        <thought>PP_SuccubiRevered_SuccubusLovin</thought>
      </li>
      <li Class="Personality.PreceptComp_SelfTookMemoryThoughtWithSubject">
        <eventDef>PP_SeductionWilling</eventDef>
        <thought>PP_SuccubiRevered_Social_SeduceAccepted</thought>
      </li>
      <li Class="Personality.PreceptComp_SelfTookMemoryThoughtWithSubject">
        <eventDef>PP_SeductionResistFailed</eventDef>
        <thought>PP_SuccubiRevered_Social_SeduceResistFailed</thought>
      </li>
      <li Class="Personality.PreceptComp_SelfTookMemoryThoughtWithSubject">
        <eventDef>PP_SeductionResisted</eventDef>
        <thought>PP_SuccubiRevered_Social_SeduceResisted</thought>
      </li>
    </comps>
  </PreceptDef>
  <PreceptDef>
    <defName>PP_SuccubiDespised</defName>
    <issue>PP_SuccubiIssue</issue>
    <label>despised</label>
    <description>Succubi are monstrous, and must not be permitted in our communities.</description>
    <comps>
      <li Class="PreceptComp_SituationalThought">
        <thought>PP_SuccubiDespised_Opinion</thought>
        <description>Succubus</description>
      </li>
      <li Class="PreceptComp_SelfTookMemoryThought">
        <eventDef>PP_GotLovinSuccubus</eventDef>
        <thought>PP_SuccubiDespised_SuccubusLovin</thought>
      </li>
    </comps>
  </PreceptDef>


</Defs>