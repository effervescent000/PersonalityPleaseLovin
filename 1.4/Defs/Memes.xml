    <generalRules>
      <rulesStrings>
        <li>memeAdjective->seductive</li>
        <li>memeAdjective->lustful</li>
        <li>memeAdjective->vital</li>
        <li>memeAdjective->decadent</li>
        <li>memeCreed->boudoir</li>
        <li>memeCreed->den</li>
        <li>memeCreed->harem</li>
        <li>memeHyphenPrefix->lust</li>
        <li>memeConcept->desire</li>
        <li>memeConcept->passion</li>
        <li>memeConcept->pleasure</li>
        <li>memeLeaderNoun->mistress</li>
